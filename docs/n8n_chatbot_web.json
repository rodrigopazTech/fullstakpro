              "value": "={{ $json.body.user?.name || 'Usuario' }}",
              "type": "string"
            },
            {
              "id": "phone",
              "name": "telefono",
              "value": "={{ $json.body.user?.phone || '' }}",
              "type": "string"
            },
            {
              "id": "message",
              "name": "mensaje",
              "value": "={{ $json.body.message }}",
              "type": "string"
            },
            {
              "id": "source",
              "name": "fuente",
